{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/renderer/src/*"
      ],
      "@view/*": [
        "src/renderer/src/view/*"
      ],
      "@desktop/*": [
        "src/renderer/src/view/desktop/*"
      ],
      "@mobile/*": [
        "src/renderer/src/view/mobile/*"
      ],
      "@hooks/*": [
        "src/renderer/src/hooks/*"
      ],
      "@store/*": [
        "src/renderer/src/store/*"
      ],
      "@components/*": [
        "src/renderer/src/components/*"
      ],
      "@assets/*": [
        "src/renderer/src/assets/*"
      ],
    }
  }
}
