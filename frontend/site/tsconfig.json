{
  "files": [],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/*"
      ],
      "@view/*": [
        "src/view/*"
      ],
      "@desktop/*": [
        "src/view/desktop/*"
      ],
      "@mobile/*": [
        "src/view/mobile/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
    },
  },
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ]
}